source :rubygems

gem "async-rack" # Collection of async-proof rack middlewares - https://github.com/rkh/async-rack.git
gem "cramp" # for everything
gem "erubis"  # for rending simple views
gem "haml" # for views
gem "http_router" # Rack based routing  
gem "json" # for to_json shit
gem "ohm" # for redis
gem "thin" # Async webserver for running a cramp application
gem "tilt" # Generic interface to multiple Ruby template engines - https://github.com/rtomayko/tilt
gem "SystemTimer"
gem "tweetstream" # duh
gem "yajl-ruby" # for tweetstream
